<ul class="sg-colors">

    {{# items }}
    <li>
        <span class="sg-swatch" style="background: {{ value }};"></span>
        <span class="sg-label">
            {{ name }}
            <br /> {{ value }}
            <br /> {{ comment }}
        </span>
    </li>
    {{/ items }}

</ul>

<small>{{{ meta.description }}}</small>
